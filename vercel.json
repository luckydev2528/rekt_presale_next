{
  "rewrites": [
    {
      "source": "/staking",
      "destination": "/staking"
    },
    {
      "source": "/loss-claim", 
      "destination": "/loss-claim"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
